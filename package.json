{
  "name": "settings-ui",
  "version": "0.0.1",
  "description": "Dashboard to manage settings API based on NextJs",
  "private": true,
  "scripts": {
    "server": "cd json-db && json-server --watch db.json --port 3004 --middlewares file.js",
    "dev": "concurrently \"npm run server\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint . --ext .tsx,*.ts --fix",
    "test": "jest",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest"
  },
  "dependencies": {
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.12",
    "next": "^12.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.24",
    "concurrently": "^5.2.0",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.1",
    "eslint-config-google": "^0.14.0",
    "jest": "^29.3.0",
    "jest-fetch-mock": "^3.0.3",
    "typescript": "^4.8.4",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2"
  }
}
