{
  "name": "settings-ui",
  "version": "0.0.1",
  "description": "Dashboard to manage settings API based on NextJs",
  "private": true,
  "scripts": {
    "server": "cd json-db && json-server db.json --port 3004 --middlewares file.js",
    "dev": "cross-env ENV_FILE=envs/.env.development next dev\"",
    "dev:staging": "cross-env ENV_FILE=envs/.env.staging next dev\"",
    "build:development": "cross-env ENV_FILE=envs/.env.development next build",
    "start:development": "cross-env ENV_FILE=envs/.env.development next start",
    "build:staging": "ENV_FILE=envs/.env.staging next build",
    "start:staging": "ENV_FILE=envs/.env.staging next start",
    "build:production": "ENV_FILE=envs/.env.production next build",
    "start:production": "ENV_FILE=envs/.env.production next start",
    "eslint": "next lint --dir .",
    "eslint:fix": "next lint . --ext .tsx --fix",
    "test": "jest",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.6",
    "@mui/material": "^5.14.6",
    "@mui/x-data-grid": "^6.11.2",
    "@rjsf/core": "^5.13.0",
    "@rjsf/utils": "^5.13.0",
    "@rjsf/validator-ajv8": "^5.13.0",
    "@types/jest": "^29.5.4",
    "dotenv": "^16.3.1",
    "next": "^13.4.19",
    "pino": "^8.15.0",
    "pino-http": "^8.4.0",
    "pino-pretty": "^10.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.2",
    "webpack-strip-code": "^1.0.3"
  },
  "devDependencies": {
    "@rjsf/mui": "^5.13.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "^13.4.19",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "next-router-mock": "^0.9.9",
    "typescript": "^5.2.2"
  }
}
